{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#1e1e1e",
    "bgPanel": "#252526",
    "bgElement": "#2d2d2d",
    "fg": "#d4d4d4",
    "fgMuted": "#808080",
    "selection": "#d7ba7d",
    "cursor": "#d4d4d4",
    "black": "#000000",
    "brightBlack": "#808080",
    "red": "#d16969",
    "brightRed": "#ce9178",
    "green": "#608b4e",
    "brightGreen": "#b5cea8",
    "yellow": "#d7ba7d",
    "brightYellow": "#dcdcaa",
    "blue": "#569cd6",
    "brightBlue": "#9cdcfe",
    "magenta": "#c586c0",
    "brightMagenta": "#c586c0",
    "cyan": "#4ec9b0",
    "brightCyan": "#4ec9b0",
    "white": "#d4d4d4",
    "brightWhite": "#ffffff"
  },
  "theme": {
    "primary": {
      "dark": "blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "cyan",
      "light": "cyan"
    },
    "accent": {
      "dark": "brightBlue",
      "light": "brightBlue"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "cyan",
      "light": "cyan"
    },
    "text": {
      "dark": "fg",
      "light": "black"
    },
    "textMuted": {
      "dark": "fgMuted",
      "light": "brightBlack"
    },
    "background": {
      "dark": "bg",
      "light": "brightWhite"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "white"
    },
    "backgroundElement": {
      "dark": "bgElement",
      "light": "fg"
    },
    "border": {
      "dark": "#3c3c3c",
      "light": "#d4d4d4"
    },
    "borderActive": {
      "dark": "blue",
      "light": "blue"
    },
    "borderSubtle": {
      "dark": "#2d2d2d",
      "light": "#e5e5e5"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "fgMuted",
      "light": "brightBlack"
    },
    "diffHunkHeader": {
      "dark": "magenta",
      "light": "magenta"
    },
    "diffHighlightAdded": {
      "dark": "brightGreen",
      "light": "brightGreen"
    },
    "diffHighlightRemoved": {
      "dark": "brightRed",
      "light": "brightRed"
    },
    "diffAddedBg": {
      "dark": "#2d3b2d",
      "light": "#e6ffec"
    },
    "diffRemovedBg": {
      "dark": "#3b2d2d",
      "light": "#ffebe9"
    },
    "diffContextBg": {
      "dark": "bgPanel",
      "light": "white"
    },
    "diffLineNumber": {
      "dark": "fgMuted",
      "light": "brightBlack"
    },
    "diffAddedLineNumberBg": {
      "dark": "#2d3b2d",
      "light": "#e6ffec"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#3b2d2d",
      "light": "#ffebe9"
    },
    "markdownText": {
      "dark": "fg",
      "light": "black"
    },
    "markdownHeading": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownLinkText": {
      "dark": "brightBlue",
      "light": "blue"
    },
    "markdownCode": {
      "dark": "brightRed",
      "light": "brightRed"
    },
    "markdownBlockQuote": {
      "dark": "brightGreen",
      "light": "green"
    },
    "markdownEmph": {
      "dark": "brightYellow",
      "light": "yellow"
    },
    "markdownStrong": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownHorizontalRule": {
      "dark": "fgMuted",
      "light": "brightBlack"
    },
    "markdownListItem": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "brightGreen",
      "light": "green"
    },
    "markdownImage": {
      "dark": "magenta",
      "light": "magenta"
    },
    "markdownImageText": {
      "dark": "brightBlue",
      "light": "blue"
    },
    "markdownCodeBlock": {
      "dark": "fg",
      "light": "black"
    },
    "syntaxComment": {
      "dark": "green",
      "light": "green"
    },
    "syntaxKeyword": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxFunction": {
      "dark": "brightYellow",
      "light": "brightYellow"
    },
    "syntaxVariable": {
      "dark": "brightBlue",
      "light": "brightBlue"
    },
    "syntaxString": {
      "dark": "brightRed",
      "light": "brightRed"
    },
    "syntaxNumber": {
      "dark": "brightGreen",
      "light": "brightGreen"
    },
    "syntaxType": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxOperator": {
      "dark": "fg",
      "light": "black"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "black"
    }
  }
}
